{
    "name": "@java-patterns/contributors",
    "version": "0.0.0",
    "description": "Java Design Patterns contributors",
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "keywords": [
        "contributors"
    ],
    "homepage": "https://github.com/AlexRogalskiy/java-patterns/tree/master/packages/contributors",
    "bugs": {
        "url": "https://github.com/AlexRogalskiy/java-patterns/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexRogalskiy/java-patterns.git",
        "directory": "packages/contributors"
    },
    "funding": "https://github.com/sponsors/AlexRogalskiy",
    "license": "GPL-3.0",
    "author": {
        "name": "Alexander Rogalskiy",
        "url": "https://github.com/AlexRogalskiy"
    },
    "maintainers": [
        {
            "name": "Alexander Rogalskiy",
            "email": "hi@sensiblemetrics.io"
        }
    ],
    "contributors": [
        {
            "name": "Alexander Rogalskiy",
            "email": "hi@sensiblemetrics.io"
        }
    ],
    "scripts": {
        "prepublishOnly": "npm run build",
        "dev": "IMAGE_DIR=$npm_config_image_dir node dist/index.js",
        "local": "npm run dev --image_dir=images",
        "build": "env-cmd --silent -e dev tsdx build",
        "watch": "env-cmd --silent -e dev tsdx watch",
        "lint:lockfile": "env-cmd --silent -e dev lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm yarn",
        "lint:report": "env-cmd --silent -e dev tsdx lint src --report-file report.json",
        "test:html": "env-cmd --silent -e test jest-html",
        "test:ci:coverage": "env-cmd --silent -e ci npm test -- --coverage --ci --testResultsProcessor=\"jest-sonar-reporter\" --watchAll=false",
        "test:coverage": "env-cmd --silent -e test jest --forceExit --runInBand --collectCoverage --detectOpenHandles --coverage && coveralls < ./coverage/lcov.info",
        "test:watch": "env-cmd --silent -e test jest --watch --notify --notifyMode=change",
        "test": "env-cmd --silent -e test jest --verbose --maxWorkers=50% --passWithNoTests --no-cache --detectOpenHandles --testPathIgnorePatterns integration local-integration disconnection",
        "test:integration": "env-cmd --silent -e test jest --runInBand --testPathIgnorePatterns disconnection --detectOpenHandles --verbose true",
        "test:local": "env-cmd --silent -e test jest --runInBand --verbose true --detectOpenHandles local-integration",
        "test:reporter": "env-cmd --silent -e test jest --ci --reporters='default' --reporters='./jest/reporter/json-reporter'",
        "test:disconnect": "env-cmd --silent -e test jest --runInBand --verbose true --detectOpenHandles disconnection"
    },
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "test-report.xml",
        "indent": 4
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/puppeteer": "^5.4.5",
        "await-sleep": "^0.0.1",
        "env-cmd": "^10.1.0",
        "jest": "^27.5.1",
        "jest-circus": "^27.5.1",
        "jest-environment-node-debug": "^2.0.0",
        "jest-extended": "^2.0.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-html": "^1.5.0",
        "jest-junit": "^13.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "jest-watch-select-projects": "^2.0.0",
        "jest-watch-typeahead": "^1.0.0",
        "lockfile-lint": "^4.7.4",
        "loglevel": "^1.8.0",
        "mockdate": "^3.0.5",
        "puppeteer": "^13.5.1",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "tsdx": "^0.14.1",
        "typescript": "^4.6.2"
    }
}
