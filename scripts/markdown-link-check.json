{
    "fallbackRetryDelay": "30s",
    "retryCount": 5,
    "retryOn429": true,
    "httpHeaders": [
        {
            "urls": [
                "https://help.github.com"
            ],
            "headers": {
                "Accept-Encoding": "br, gzip, deflate"
            }
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/",
            "replacement": "/github/workspace/"
        }
    ],
    "ignorePatterns": [
        {
            "pattern": "^http[s]*://localhost.*"
        }
    ]
}
